#: shippings/ibenic_woo_hp_shipping.php:105
msgid "International - Ransom (Cash on Delivery)"
msgstr "Međunarodno - otkupnina"

#: shippings/ibenic_woo_hp_shipping.php:107
msgid "Using ransom when shipping International."
msgstr "Koristi otkupninu prilikom međunarodnog dostavljanja"

#: shippings/ibenic_woo_hp_shipping.php:111
msgid "International - Returnee (Advice of Delivery)"
msgstr "Međunarodno - povratnica"

#: shippings/ibenic_woo_hp_shipping.php:113
msgid "Using returnee when shipping International."
msgstr "Koristi povratnicu prilikom međunarodnog dostavljanja"

